// lambda expression
public class Main
{
	public static void main(String[] args) {
	    Thread obj1=new Thread(() ->{
	       
	            Thread.currentThread().setName("aashi");
	            for(int i=0;i<10;i++){
	                System.out.println(Thread.currentThread().getName());
	            
	        }
	    });
	    obj1.start();
	    for(int i=0;i<10;i++){
		System.out.println("Hello World");
	    }
	}
}
